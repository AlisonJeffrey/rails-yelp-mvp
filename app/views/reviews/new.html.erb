<div class="container">
  <h1>Add a review</h1>
  <hr>
    <%= simple_form_for [@restaurant, @review] do |f| %>
    <%= f.input :rating %>
    <%= f.input :content %>
    <%= f.submit "Submit review", class: "btn btn-light" %>
  <% end %>
  <hr>
  <%= link_to 'Back to Restaurants', restaurants_path %>
</div>
